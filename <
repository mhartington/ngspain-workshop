  templateUrl: 'home.page.html',
  styleUrls: ['home.page.scss'],
})
export class HomePage {

  constructor(private alertCtrl: AlertController) {}
  showAlert(){
    const alert = this.alertCtrl.create({
        message: "Clicked Delete"
    })

  }
}
